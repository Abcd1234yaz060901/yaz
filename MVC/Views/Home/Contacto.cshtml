
<!--inicio de contactanos-->
    
<div class="container">
    <div style="text-align:center">
        <br /><br /><br /><br /><br /><br />
        <h2>Contact Us</h2>
        <p>Swing by for a cup of coffee, or leave us a message:</p>
    </div>

    <section class="col-md-5" style="text-align:left">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3785.095907172777!2d-68.99604988630026!3d18.43395407663143!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8eaf541bb9c77e59%3A0x341ce695e435fe53!2sCaribbean%20Cinemas!5e0!3m2!1ses!2sdo!4v1569208987423!5m2!1ses!2sdo" width="600" height="450" frameborder="0" style="border:0;"></iframe>
    </section>

    <section class="col-md-5" style="text-align:right">

        <h2>Contact Us</h2>
        <p>@ViewBag.Direccion</p>
        <p>@ViewBag.Pais</p>

        <form role="form">
            <div class="form-group">
                <label for="email">Email address:</label>
                <input style="margin-left:170px" type="email" class="form-control" id="email">
            </div>
            <div class="form-group">
                <label for="pwd">Password:</label>
                <input type="password" style="margin-left:170px" class="form-control" id="pwd">
            </div>
            <div class="checkbox">
                <label><input type="checkbox"> Remember me</label>
            </div>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>

    </section>
    <!--fin contactanos-->
